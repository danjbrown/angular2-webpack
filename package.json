{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "server": "nodemon ./src/server/index.js",
    "prod": "node ./dist/production_server.js",
    "test:client": "karma start",
    "test:server": "mocha src/server/services/*.spec.js",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail && cp production_server.js dist",
    "lint:client": "yarn run tslint \"src/client/**/*.ts\"",
    "lint:server": "jshint src/server"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/danjbrown/angular2-webpack/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "@angular/common": "~2.1.1",
    "@angular/compiler": "~2.1.1",
    "@angular/core": "~2.1.1",
    "@angular/forms": "~2.1.1",
    "@angular/http": "~2.1.1",
    "@angular/platform-browser": "~2.1.1",
    "@angular/platform-browser-dynamic": "~2.1.1",
    "@angular/router": "~3.1.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.3",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "log4js": "^2.5.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.4.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.9.5",
    "jslint": "^0.12.0",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^5.0.5",
    "node-sass": "^4.8.3",
    "nodemon": "^1.13.3",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.13.1",
    "ts-loader": "^3.2.0",
    "tslint": "^4.0.2",
    "typescript": "^2.6.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
